{
  "name": "advocacy.mozilla.org",
  "version": "1.0.0",
  "description": "advocacy.mozilla.org",
  "scripts": {
    "start": "npm-run-all --parallel server watch:js watch:css",
    "build:production": "npm-run-all --parallel minify:js minify:css",
    "build:dev": "npm-run-all --parallel build:js build:css",
    "build:js": "webpack --progress --profile --colors",
    "minify:js": "npm run build:js -- --optimize-minimize --optimize-dedupe",
    "minify:css": "npm run watch:css -- --no-watch",
    "watch:js": "npm run build:js -- --watch",
    "server": "node app.js",
    "postinstall": "node build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/advocacy.mozilla.org"
  },
  "keywords": [
    "advocacy"
  ],
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/advocacy.mozilla.org/issues"
  },
  "homepage": "https://github.com/mozilla/advocacy.mozilla.org",
  "dependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "compression": "1.6.1",
    "cross-spawn": "0.2.9",
    "css-loader": "0.14.2",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.4",
    "habitat": "3.1.2",
    "json-loader": "0.5.1",
    "jsx-loader": "0.13.2",
    "less": "2.5.3",
    "less-loader": "2.2.2",
    "npm-run-all": "1.2.4",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-hot-loader": "1.2.7",
    "react-router": "2.0.0-rc5",
    "style-loader": "0.13.0",
    "url-loader": "0.5.6",
    "webpack": "1.12.12"
  }
}
