{
  "name": "advocacy.mozilla.org",
  "version": "1.0.0",
  "description": "advocacy.mozilla.org",
  "scripts": {
    "start": "node scripts/build.js",
    "start:prod": "npm-run-all build:*",
    "start:dev": "npm-run-all build:babel start:watch",
    "start:watch": "npm-run-all --parallel build:l10njson watch:js server",
    "build:l10njson": "node scripts/properties2json",
    "build:babel": "babel src --compact=true -s --out-dir dist",
    "build:webpack": "webpack --optimize-minimize --optimize-dedupe",
    "watch:js": "npm-run-all --parallel watch:webpack watch:babel",
    "watch:babel": "npm run build:babel -- -w -q",
    "watch:webpack": "webpack --progress --profile --colors --watch",
    "server": "node app.js",
    "lint": "eslint --config ./node_modules/mofo-style/.eslintrc.yaml --ext .js .",
    "lintfix": "eslint --config ./node_modules/mofo-style/.eslintrc.yaml --ext .js . --fix",
    "postinstall": "npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/advocacy.mozilla.org"
  },
  "keywords": [
    "advocacy"
  ],
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/advocacy.mozilla.org/issues"
  },
  "homepage": "https://github.com/mozilla/advocacy.mozilla.org",
  "dependencies": {
    "accept-language-parser": "1.0.2",
    "async": "1.5.2",
    "autoprefixer": "6.3.3",
    "babel-cli": "6.7.5",
    "babel-core": "^6.5.2",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "bestlang": "0.0.1",
    "classnames": "2.2.3",
    "compression": "1.6.1",
    "cross-spawn": "0.2.9",
    "css-loader": "0.14.2",
    "eslint-plugin-react": "3.16.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.4",
    "fs-writefile-promise": "1.0.3",
    "habitat": "3.1.2",
    "helmet": "1.3.0",
    "hoek": "3.0.4",
    "intl": "^1.2.4",
    "json-loader": "0.5.1",
    "jsx-loader": "0.13.2",
    "langmap": "0.0.12",
    "less": "2.5.3",
    "less-loader": "2.2.2",
    "mozilla-tabzilla": "^0.5.1",
    "npm-run-all": "1.2.4",
    "postcss-loader": "0.8.1",
    "properties-parser": "0.3.1",
    "q-io": "1.13.2",
    "react": "0.14.6",
    "react-addons-linked-state-mixin": "0.14.6",
    "react-dom": "0.14.6",
    "react-ga": "1.2.0",
    "react-hot-loader": "1.2.7",
    "react-intl": "^2.1.2",
    "react-router": "2.0.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.6",
    "webpack": "1.12.12"
  },
  "devDependencies": {
    "eslint": "1.10.3",
    "mofo-style": "2.2.0"
  }
}
